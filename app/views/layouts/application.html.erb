<!DOCTYPE html>
<html>
<head>
  <title>HeyLaundry</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
  <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="social-buttons.css">

	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta property="og:type" content="website" />
	<meta property="og:title" content="heylaundry"/>
	<meta property="og:url" content="http://heylaundry.com/"/>
	<meta property="og:image" content="<%= asset_path('fb-share.png') %>"/>
	<meta property="og:description" content="Your dirty laundry washed and hand-delivered right to your doorstep." />
	<meta property="og:site_name" content="heylaundry.com"/>

	<meta name="title" content="heylaundry"/>
	<meta name="description" content="Your dirty laundry washed and hand-delivered right to your doorstep." />

	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>

</head>
<body>
	<script>
	 window.fbAsyncInit = function() {
	 FB.init({
	 appId : '554636974639315',
	 xfbml : true,
	 version : 'v2.2'
	 });
	 };

	 (function(d, s, id){
	 var js, fjs = d.getElementsByTagName(s)[0];
	 if (d.getElementById(id)) {return;}
	 js = d.createElement(s); js.id = id;
	 js.src = "//connect.facebook.net/en_US/sdk.js";
	 fjs.parentNode.insertBefore(js, fjs);
	 }(document, 'script', 'facebook-jssdk'));
	</script>
<%= yield %>

</body>
</html>